{
  "[dockercompose]": {
    "editor.autoIndent": "advanced",
    "editor.defaultFormatter": "redhat.vscode-yaml",
    "editor.insertSpaces": true,
    "editor.quickSuggestions": {
      "comments": false,
      "other": true,
      "strings": true
    },
    "editor.tabSize": 2
  },
  "[github-actions-workflow]": {
    "editor.defaultFormatter": "redhat.vscode-yaml"
  },
  "chat.agent.enabled": false,
  "chat.commandCenter.enabled": false,
  "chat.editor.fontSize": 14,
  "chat.extensionUnification.enabled": false,
  "continue.enableTabAutocomplete": true,
  "continue.telemetryEnabled": false,
  "debug.console.fontSize": 14,
  "dev.containers.cacheVolume": false,
  "dev.containers.defaultExtensions": [
    "oderwat.indent-rainbow"
  ],
  "dev.containers.workspaceMountConsistency": "consistent",
  "docsYaml.telemetry.enableTelemetry": false,
  "dotfiles.repository": "https://github.com/johnko/devcontainer-dotfiles.git",
  "editor.fontSize": 14,
  "editor.minimap.enabled": true,
  "editor.minimap.renderCharacters": false,
  "editor.renderWhitespace": "all",
  "editor.stickyScroll.enabled": true,
  "editor.tabSize": 2,
  "editor.wordWrap": "on",
  "explorer.excludeGitIgnore": true,
  "extensions.ignoreRecommendations": true,
  "files.insertFinalNewline": true,
  "files.trimTrailingWhitespace": true,
  "git.openRepositoryInParentFolders": "never",
  "git.replaceTagsWhenPull": true,
  "github.copilot.chat.backgroundAgent.enabled": false,
  "github.copilot.chat.cloudAgent.enabled": false,
  "github.copilot.editor.enableAutoCompletions": false,
  "github.copilot.enable": {
    "*": false
  },
  "github.gitProtocol": "ssh",
  "githubPullRequests.defaultCommentType": "single",
  "githubPullRequests.defaultCreateOption": "createDraft",
  "githubPullRequests.defaultMergeMethod": "squash",
  "githubPullRequests.devMode": true,
  "githubPullRequests.focusedMode": false,
  "githubPullRequests.hideViewedFiles": true,
  "githubPullRequests.ignoreSubmodules": true,
  "githubPullRequests.ignoredPullRequestBranches": [
    "main",
    "master"
  ],
  "githubPullRequests.notifications": "pullRequests",
  "githubPullRequests.pullBranch": "always",
  "githubPullRequests.pushBranch": "always",
  "githubPullRequests.quickDiff": true,
  "githubPullRequests.showPullRequestNumberInTree": true,
  "githubPullRequests.useReviewMode": {
    "closed": false,
    "merged": false
  },
  "go.survey.prompt": false,
  "inlineChat.holdToSpeech": false,
  "markdownlint.customRules": [
    "{docsmsft.docs-linting}/markdownlint-custom-rules/rules.js"
  ],
  "peacock.color": "#cc0000",
  "peacock.favoriteColors": [
    {
      "name": "black",
      "value": "#222222"
    },
    {
      "name": "white",
      "value": "#eeeeee"
    },
    {
      "name": "silver",
      "value": "gray"
    },
    {
      "name": "red",
      "value": "#cc0000"
    },
    {
      "name": "green",
      "value": "green"
    },
    {
      "name": "yellow",
      "value": "yellow"
    },
    {
      "name": "blue",
      "value": "#0040ff"
    },
    {
      "name": "purple",
      "value": "#893de9"
    },
    {
      "name": "magenta",
      "value": "#ff00ff"
    },
    {
      "name": "orange",
      "value": "#ff8000"
    },
    {
      "name": "cyan",
      "value": "cyan"
    }
  ],
  "peacock.surpriseMeFromFavoritesOnly": true,
  "pgsql.resultsFontSize": 14,
  "python.defaultInterpreterPath": "~/venv-python3",
  "redhat.telemetry.enabled": false,
  "remote.SSH.enableAgentForwarding": false,
  "remote.SSH.experimental.chat": false,
  "remote.defaultExtensionsIfInstalledLocally": [
    "GitHub.vscode-pull-request-github"
  ],
  "remoteHub.gitProtocol": "ssh",
  "scm.diffDecorationsIgnoreTrimWhitespace": "true",
  "scm.inputFontSize": 14,
  "settingsSync.ignoredExtensions": [
    "github.copilot-chat",
    "github.copilot"
  ],
  "settingsSync.ignoredSettings": [
    "-dev.containers.copyGitConfig",
    "-dotfiles.installCommand",
    "-dotfiles.repository",
    "-dotfiles.targetPath",
    "-python.defaultInterpreterPath"
  ],
  "telemetry.telemetryLevel": "off",
  "terminal.explorerKind": "external",
  "terminal.integrated.defaultLocation": "editor",
  "terminal.integrated.fontSize": 14,
  "terminal.integrated.hideOnStartup": "whenEmpty",
  "terminal.integrated.inheritEnv": false,
  "terminal.integrated.initialHint": false,
  "terraform.codelens.referenceCount": true,
  "terraform.experimentalFeatures.prefillRequiredFields": true,
  "terraform.experimentalFeatures.validateOnSave": true,
  "typescript.surveys.enabled": false,
  "update.showReleaseNotes": false,
  "window.commandCenter": false,
  "window.confirmSaveUntitledWorkspace": false,
  "window.newWindowProfile": "Default",
  "window.title": "${activeRepositoryBranchName}${separator}${activeEditorShort}${separator}${rootName}${separator}${profileName}",
  "workbench.colorCustomizations": {
    "activityBar.activeBackground": "#ff0000",
    "activityBar.background": "#ff0000",
    "activityBar.foreground": "#e7e7e7",
    "activityBar.inactiveForeground": "#e7e7e799",
    "activityBarBadge.background": "#00df00",
    "activityBarBadge.foreground": "#15202b",
    "commandCenter.border": "#e7e7e799",
    "sash.hoverBorder": "#ff0000",
    "statusBar.background": "#cc0000",
    "statusBar.foreground": "#e7e7e7",
    "statusBarItem.hoverBackground": "#ff0000",
    "statusBarItem.remoteBackground": "#cc0000",
    "statusBarItem.remoteForeground": "#e7e7e7",
    "titleBar.activeBackground": "#cc0000",
    "titleBar.activeForeground": "#e7e7e7",
    "titleBar.inactiveBackground": "#cc000099",
    "titleBar.inactiveForeground": "#e7e7e799"
  },
  "workbench.colorTheme": "Visual Studio Light",
  "workbench.editor.empty.hint": "hidden",
  "workbench.editor.enablePreview": false,
  "workbench.editor.scrollToSwitchTabs": true,
  "workbench.editor.showTabIndex": true,
  "workbench.editor.tabActionCloseVisibility": false,
  "workbench.editor.wrapTabs": true,
  "workbench.preferredDarkColorTheme": "Visual Studio Dark",
  "workbench.preferredLightColorTheme": "Visual Studio Light",
  "workbench.secondarySideBar.showLabels": false,
  "workbench.settings.applyToAllProfiles": [
    "[dockercompose]",
    "[github-actions-workflow]",
    "chat.agent.enabled",
    "chat.commandCenter.enabled",
    "chat.editor.fontSize",
    "chat.extensionUnification.enabled",
    "continue.enableTabAutocomplete",
    "continue.telemetryEnabled",
    "debug.console.fontSize",
    "dev.containers.cacheVolume",
    "dev.containers.defaultExtensions",
    "dev.containers.workspaceMountConsistency",
    "docsYaml.telemetry.enableTelemetry",
    "dotfiles.repository",
    "editor.fontSize",
    "editor.minimap.enabled",
    "editor.minimap.renderCharacters",
    "editor.renderWhitespace",
    "editor.stickyScroll.enabled",
    "editor.tabSize",
    "editor.wordWrap",
    "explorer.excludeGitIgnore",
    "extensions.ignoreRecommendations",
    "files.insertFinalNewline",
    "files.trimTrailingWhitespace",
    "git.openRepositoryInParentFolders",
    "git.replaceTagsWhenPull",
    "github.copilot.chat.backgroundAgent.enabled",
    "github.copilot.chat.cloudAgent.enabled",
    "github.copilot.editor.enableAutoCompletions",
    "github.copilot.enable",
    "github.gitProtocol",
    "githubPullRequests.defaultCommentType",
    "githubPullRequests.defaultCreateOption",
    "githubPullRequests.defaultMergeMethod",
    "githubPullRequests.devMode",
    "githubPullRequests.focusedMode",
    "githubPullRequests.hideViewedFiles",
    "githubPullRequests.ignoreSubmodules",
    "githubPullRequests.ignoredPullRequestBranches",
    "githubPullRequests.notifications",
    "githubPullRequests.pullBranch",
    "githubPullRequests.pushBranch",
    "githubPullRequests.quickDiff",
    "githubPullRequests.showPullRequestNumberInTree",
    "githubPullRequests.useReviewMode",
    "go.survey.prompt",
    "inlineChat.holdToSpeech",
    "markdownlint.customRules",
    "peacock.favoriteColors",
    "peacock.surpriseMeFromFavoritesOnly",
    "pgsql.resultsFontSize",
    "redhat.telemetry.enabled",
    "remote.SSH.enableAgentForwarding",
    "remote.SSH.experimental.chat",
    "remote.defaultExtensionsIfInstalledLocally",
    "remoteHub.gitProtocol",
    "scm.diffDecorationsIgnoreTrimWhitespace",
    "scm.inputFontSize",
    "settingsSync.ignoredExtensions",
    "settingsSync.ignoredSettings",
    "telemetry.telemetryLevel",
    "terminal.explorerKind",
    "terminal.integrated.defaultLocation",
    "terminal.integrated.fontSize",
    "terminal.integrated.hideOnStartup",
    "terminal.integrated.inheritEnv",
    "terminal.integrated.initialHint",
    "terraform.codelens.referenceCount",
    "terraform.experimentalFeatures.prefillRequiredFields",
    "terraform.experimentalFeatures.validateOnSave",
    "typescript.surveys.enabled",
    "update.showReleaseNotes",
    "window.commandCenter",
    "window.confirmSaveUntitledWorkspace",
    "window.newWindowProfile",
    "window.title",
    "workbench.colorTheme",
    "workbench.editor.empty.hint",
    "workbench.editor.enablePreview",
    "workbench.editor.scrollToSwitchTabs",
    "workbench.editor.showTabIndex",
    "workbench.editor.tabActionCloseVisibility",
    "workbench.editor.wrapTabs",
    "workbench.preferredDarkColorTheme",
    "workbench.preferredLightColorTheme",
    "workbench.secondarySideBar.showLabels",
    "workbench.settings.applyToAllProfiles",
    "workbench.startupEditor",
    "yaml.schemas"
  ],
  "workbench.startupEditor": "none",
  "yaml.schemas": {
    "file://~/.vscode/extensions/continue.continue-1.2.14-darwin-arm64/config-yaml-schema.json": [
      ".continue/**/*.yaml"
    ]
  }
}
