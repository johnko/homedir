{
  // See https://go.microsoft.com/fwlink/?LinkId=733558
  // for the documentation about the tasks.json format
  "inputs": [
    {
      "default": "lab",
      "description": "Select a repo:",
      "id": "repo1",
      "options": [
        "devcontainer-dotfiles",
        "dockprom",
        "encrypt-message-to-github-user",
        "homedir",
        "iac-github",
        "lab",
        "ollama-code",
        "opencode-devcontainers",
        "renovate-config",
        "terraform-aws-eks",
        "terraform-aws-eks-blueprints"
      ],
      "type": "pickString"
    },
    {
      "default": "chore/",
      "description": "Name of branch",
      "id": "branch1",
      "type": "promptString"
    }
  ],
  "tasks": [
    {
      "command": "osascript -e 'tell application \"Terminal\"\n  set newTab to do script (\"gg clone ${input:repo1} ${input:branch1}\")\nend tell'",
      "label": "gg clone local-repo branch",
      "presentation": {
        "echo": true,
        "reveal": "never",
        "focus": false,
        "panel": "dedicated",
        "showReuseMessage": true,
        "clear": false
      },
      "problemMatcher": [],
      "type": "shell"
    }
  ],
  "version": "2.0.0"
}
