{
  "[dockercompose]": {
    "editor.autoIndent": "advanced",
    "editor.defaultFormatter": "redhat.vscode-yaml",
    "editor.insertSpaces": true,
    "editor.quickSuggestions": {
      "comments": false,
      "other": true,
      "strings": true
    },
    "editor.tabSize": 2
  },
  "[github-actions-workflow]": {
    "editor.defaultFormatter": "redhat.vscode-yaml"
  },
  "accessibility.openChatEditedFiles": true,
  "aws.cloudformation.telemetry.enabled": false,
  "aws.telemetry": false,
  "chat.agent.enabled": false,
  "chat.commandCenter.enabled": false,
  "chat.editor.fontSize": 14,
  "chat.extensionUnification.enabled": false,
  "comments.visible": false,
  "containers.containerClient": "com.microsoft.visualstudio.containers.podman",
  "containers.orchestratorClient": "com.microsoft.visualstudio.orchestrators.podmancompose",
  "continue.enableTabAutocomplete": true,
  "continue.telemetryEnabled": false,
  "debug.console.fontSize": 14,
  "debug.terminal.clearBeforeReusing": true,
  "dev.containers.cacheVolume": false,
  "dev.containers.defaultExtensions": [],
  "dev.containers.workspaceMountConsistency": "consistent",
  "docsYaml.telemetry.enableTelemetry": false,
  "dotfiles.repository": "https://github.com/johnko/devcontainer-dotfiles.git",
  "editor.acceptSuggestionOnEnter": "off",
  "editor.foldingImportsByDefault": true,
  "editor.fontSize": 14,
  "editor.inlineSuggest.showToolbar": "always",
  "editor.minimap.enabled": true,
  "editor.minimap.renderCharacters": false,
  "editor.minimap.side": "left",
  "editor.minimap.size": "fit",
  "editor.renderWhitespace": "all",
  "editor.scrollOnMiddleClick": true,
  "editor.showFoldingControls": "always",
  "editor.stickyScroll.enabled": true,
  "editor.suggest.showStatusBar": true,
  "editor.tabSize": 2,
  "editor.wordWrap": "on",
  "explorer.excludeGitIgnore": true,
  "explorer.fileNesting.enabled": true,
  "explorer.fileNesting.expand": false,
  "extensions.ignoreRecommendations": true,
  "files.insertFinalNewline": true,
  "files.trimTrailingWhitespace": true,
  "git.confirmSync": false,
  "git.openRepositoryInParentFolders": "never",
  "git.replaceTagsWhenPull": true,
  "github.copilot.chat.backgroundAgent.enabled": false,
  "github.copilot.chat.cloudAgent.enabled": false,
  "github.copilot.editor.enableAutoCompletions": false,
  "github.copilot.enable": {
    "*": false
  },
  "github.gitProtocol": "ssh",
  "githubPullRequests.assignCreated": "${user}",
  "githubPullRequests.createOnPublishBranch": "always",
  "githubPullRequests.defaultCommentType": "single",
  "githubPullRequests.defaultCreateOption": "createDraft",
  "githubPullRequests.defaultMergeMethod": "squash",
  "githubPullRequests.focusedMode": false,
  "githubPullRequests.hideViewedFiles": true,
  "githubPullRequests.ignoreSubmodules": true,
  "githubPullRequests.notifications": "pullRequests",
  "githubPullRequests.pullBranch": "always",
  "githubPullRequests.pushBranch": "always",
  "githubPullRequests.quickDiff": true,
  "githubPullRequests.showPullRequestNumberInTree": true,
  "githubPullRequests.useReviewMode": {
    "closed": false,
    "merged": false
  },
  "githubRepositories.autoFetch.interval": 180,
  "go.survey.prompt": false,
  "inlineChat.holdToSpeech": false,
  "markdownlint.customRules": [
    "{docsmsft.docs-linting}/markdownlint-custom-rules/rules.js"
  ],
  "peacock.favoriteColors": [
    {
      "name": "black",
      "value": "#222222"
    },
    {
      "name": "white",
      "value": "#eeeeee"
    },
    {
      "name": "silver",
      "value": "gray"
    },
    {
      "name": "red",
      "value": "#cc0000"
    },
    {
      "name": "green",
      "value": "green"
    },
    {
      "name": "yellow",
      "value": "yellow"
    },
    {
      "name": "blue",
      "value": "#0040ff"
    },
    {
      "name": "purple",
      "value": "#893de9"
    },
    {
      "name": "magenta",
      "value": "#ff00ff"
    },
    {
      "name": "orange",
      "value": "#ff8000"
    },
    {
      "name": "cyan",
      "value": "cyan"
    }
  ],
  "peacock.surpriseMeFromFavoritesOnly": true,
  "pgsql.resultsFontSize": 14,
  "python.defaultInterpreterPath": "/opt/homebrew/bin/python3",
  "redhat.telemetry.enabled": false,
  "remote.SSH.enableAgentForwarding": false,
  "remote.SSH.experimental.chat": false,
  "remote.defaultExtensionsIfInstalledLocally": [
    "GitHub.vscode-pull-request-github"
  ],
  "remoteHub.gitProtocol": "ssh",
  "scm.alwaysShowRepositories": true,
  "scm.defaultViewMode": "tree",
  "scm.defaultViewSortKey": "status",
  "scm.diffDecorationsIgnoreTrimWhitespace": "true",
  "scm.inputFontSize": 14,
  "scm.repositories.visible": 20,
  "search.defaultViewMode": "tree",
  "search.mode": "newEditor",
  "search.quickAccess.preserveInput": true,
  "search.searchEditor.defaultNumberOfContextLines": 3,
  "search.showLineNumbers": true,
  "search.smartCase": true,
  "settingsSync.ignoredExtensions": [
    "github.copilot-chat",
    "github.copilot"
  ],
  "settingsSync.ignoredSettings": [
    "-dotfiles.repository"
  ],
  "task.quickOpen.skip": true,
  "telemetry.feedback.enabled": false,
  "telemetry.telemetryLevel": "off",
  "terminal.integrated.cursorBlinking": true,
  "terminal.integrated.fontSize": 14,
  "terminal.integrated.hideOnStartup": "whenEmpty",
  "terminal.integrated.inheritEnv": true,
  "terminal.integrated.initialHint": false,
  "terraform.codelens.referenceCount": true,
  "terraform.experimentalFeatures.prefillRequiredFields": true,
  "terraform.experimentalFeatures.validateOnSave": true,
  "testing.automaticallyOpenPeekView": "failureAnywhere",
  "testing.automaticallyOpenTestResults": "openOnTestFailure",
  "testing.coverageToolbarEnabled": true,
  "typescript.surveys.enabled": false,
  "update.showReleaseNotes": false,
  "vs-kubernetes": {},
  "window.clickThroughInactive": false,
  "window.commandCenter": false,
  "window.confirmBeforeClose": "keyboardOnly",
  "window.confirmSaveUntitledWorkspace": false,
  "window.newWindowDimensions": "offset",
  "window.newWindowProfile": "Default",
  "window.openFilesInNewWindow": "default",
  "window.openFoldersInNewWindow": "on",
  "window.openWithoutArgumentsInNewWindow": "on",
  "window.restoreWindows": "preserve",
  "window.title": "${activeRepositoryBranchName}${separator}${activeEditorShort}${separator}${rootName}${separator}${profileName}",
  "workbench.colorTheme": "Default Light Modern",
  "workbench.editor.empty.hint": "hidden",
  "workbench.editor.enablePreview": false,
  "workbench.editor.scrollToSwitchTabs": true,
  "workbench.editor.showTabIndex": true,
  "workbench.editor.tabActionCloseVisibility": false,
  "workbench.editor.wrapTabs": true,
  "workbench.panel.defaultLocation": "top",
  "workbench.settings.applyToAllProfiles": [
    "[dockercompose]",
    "[github-actions-workflow]",
    "accessibility.openChatEditedFiles",
    "aws.cloudformation.telemetry.enabled",
    "aws.telemetry",
    "chat.agent.enabled",
    "chat.commandCenter.enabled",
    "chat.editor.fontSize",
    "chat.extensionUnification.enabled",
