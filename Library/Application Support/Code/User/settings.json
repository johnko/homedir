{
  "debug.console.fontSize": 14,
  "docsYaml.telemetry.enableTelemetry": false,
  "editor.fontSize": 14,
  "editor.renderWhitespace": "all",
  "editor.stickyScroll.enabled": true,
  "editor.tabSize": 2,
  "editor.wordWrap": "on",
  "extensions.ignoreRecommendations": true,
  "files.insertFinalNewline": true,
  "files.trimTrailingWhitespace": true,
  "git.enableCommitSigning": true,
  "github.copilot.editor.enableAutoCompletions": true,
  "github.copilot.enable": {
    "*": false,
    "ini": false,
    "markdown": false,
    "plaintext": false,
    "scminput": false,
    "terraform": true,
    "yaml": false
  },
  "githubPullRequests.defaultCreateOption": "createDraft",
  "githubPullRequests.defaultMergeMethod": "squash",
  "githubPullRequests.focusedMode": "overview",
  "githubPullRequests.ignoredPullRequestBranches": [
    "main",
    "master"
  ],
  "githubPullRequests.pullBranch": "always",
  "githubPullRequests.quickDiff": true,
  "githubPullRequests.showPullRequestNumberInTree": true,
  "githubPullRequests.useReviewMode": {
    "closed": false,
    "merged": false
  },
  "gitlens.codeLens.enabled": false,
  "go.survey.prompt": false,
  "markdownlint.config": {
    "MD025": {
      "front_matter_title": ""
    },
    "MD028": false
  },
  "markdownlint.customRules": [
    "{docsmsft.docs-linting}/markdownlint-custom-rules/rules.js"
  ],
  "peacock.favoriteColors": [
    {
      "name": "Angular Red",
      "value": "#dd0531"
    },
    {
      "name": "Azure Blue",
      "value": "#007fff"
    },
    {
      "name": "JavaScript Yellow",
      "value": "#f9e64f"
    },
    {
      "name": "Mandalorian Blue",
      "value": "#1857a4"
    },
    {
      "name": "Node Green",
      "value": "#215732"
    },
    {
      "name": "React Blue",
      "value": "#61dafb"
    },
    {
      "name": "Something Different",
      "value": "#832561"
    },
    {
      "name": "Svelte Orange",
      "value": "#ff3d00"
    },
    {
      "name": "Vue Green",
      "value": "#42b883"
    },
    {
      "name": "Purple",
      "value": "#893de9"
    },
    {
      "name": "Yellow",
      "value": "#e7d202"
    },
    {
      "name": "Green",
      "value": "#65c846"
    }
  ],
  "peacock.surpriseMeOnStartup": true,
  "pgsql.resultsFontSize": 14,
  "redhat.telemetry.enabled": false,
  "scm.inputFontSize": 14,
  "telemetry.telemetryLevel": "off",
  "terminal.integrated.defaultLocation": "editor",
  "terminal.integrated.fontSize": 14,
  "terraform.codelens.referenceCount": true,
  "terraform.experimentalFeatures.prefillRequiredFields": true,
  "terraform.experimentalFeatures.validateOnSave": true,
  "typescript.surveys.enabled": false,
  "update.showReleaseNotes": false,
  "window.autoDetectColorScheme": true,
  "workbench.editor.empty.hint": "hidden",
  "workbench.preferredDarkColorTheme": "Visual Studio Dark",
  "workbench.preferredLightColorTheme": "Visual Studio Light",
  "workbench.startupEditor": "none",
  "yaml.schemas": {
    "file://~/.vscode/extensions/docsmsft.docs-yaml-1.0.4/dist/toc.schema.json": "/toc\\.yml/i"
  }
}
