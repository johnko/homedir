{
  "[dockercompose]": {
    "editor.autoIndent": "advanced",
    "editor.defaultFormatter": "redhat.vscode-yaml",
    "editor.insertSpaces": true,
    "editor.quickSuggestions": {
      "comments": false,
      "other": true,
      "strings": true
    },
    "editor.tabSize": 2
  },
  "[github-actions-workflow]": {
    "editor.defaultFormatter": "redhat.vscode-yaml"
  },
  "chat.agent.enabled": false,
  "chat.commandCenter.enabled": false,
  "chat.disableAIFeatures": true,
  "chat.editor.fontSize": 14,
  "continue.enableTabAutocomplete": true,
  "continue.telemetryEnabled": false,
  "debug.console.fontSize": 14,
  "dev.containers.defaultExtensions": [
    "johnpapa.vscode-peacock",
    "oderwat.indent-rainbow"
  ],
  "docsYaml.telemetry.enableTelemetry": false,
  "dotfiles.installCommand": "bootstrap.sh",
  "dotfiles.repository": "https://github.com/johnko/devcontainer-dotfiles.git",
  "editor.fontSize": 14,
  "editor.renderWhitespace": "all",
  "editor.stickyScroll.enabled": true,
  "editor.tabSize": 2,
  "editor.wordWrap": "on",
  "explorer.excludeGitIgnore": true,
  "extensions.ignoreRecommendations": true,
  "files.insertFinalNewline": true,
  "files.trimTrailingWhitespace": true,
  "git.enableCommitSigning": true,
  "git.openRepositoryInParentFolders": "never",
  "git.replaceTagsWhenPull": true,
  "github.copilot.chat.backgroundAgent.enabled": false,
  "github.copilot.chat.cloudAgent.enabled": false,
  "github.copilot.editor.enableAutoCompletions": false,
  "github.copilot.enable": {
    "*": false
  },
  "github.gitProtocol": "ssh",
  "githubPullRequests.defaultCommentType": "single",
  "githubPullRequests.defaultCreateOption": "createDraft",
  "githubPullRequests.defaultMergeMethod": "squash",
  "githubPullRequests.devMode": true,
  "githubPullRequests.focusedMode": false,
  "githubPullRequests.hideViewedFiles": true,
  "githubPullRequests.ignoreSubmodules": true,
  "githubPullRequests.ignoredPullRequestBranches": [
    "main",
    "master"
  ],
  "githubPullRequests.notifications": "pullRequests",
  "githubPullRequests.pullBranch": "always",
  "githubPullRequests.pushBranch": "always",
  "githubPullRequests.quickDiff": true,
  "githubPullRequests.showPullRequestNumberInTree": true,
  "githubPullRequests.useReviewMode": {
    "closed": false,
    "merged": false
  },
  "gitlens.codeLens.enabled": false,
  "gitlens.telemetry.enabled": false,
  "go.survey.prompt": false,
  "inlineChat.holdToSpeech": false,
  "markdownlint.customRules": [
    "{docsmsft.docs-linting}/markdownlint-custom-rules/rules.js"
  ],
  "peacock.favoriteColors": [
    {
      "name": "black",
      "value": "#222222"
    },
    {
      "name": "white",
      "value": "#eeeeee"
    },
    {
      "name": "silver",
      "value": "gray"
    },
    {
      "name": "red",
      "value": "#cc0000"
    },
    {
      "name": "green",
      "value": "green"
    },
    {
      "name": "yellow",
      "value": "yellow"
    },
    {
      "name": "blue",
      "value": "#0040ff"
    },
    {
      "name": "purple",
      "value": "#893de9"
    },
    {
      "name": "magenta",
      "value": "#ff00ff"
    },
    {
      "name": "orange",
      "value": "#ff8000"
    },
    {
      "name": "cyan",
      "value": "cyan"
    }
  ],
  "peacock.surpriseMeFromFavoritesOnly": true,
  "peacock.surpriseMeOnStartup": true,
  "pgsql.resultsFontSize": 14,
  "python.defaultInterpreterPath": "~/venv-python3",
  "redhat.telemetry.enabled": false,
  "remote.SSH.enableAgentForwarding": false,
  "remoteHub.gitProtocol": "ssh",
  "scm.countBadge": "focused",
  "scm.diffDecorationsIgnoreTrimWhitespace": "true",
  "scm.inputFontSize": 14,
  "settingsSync.ignoredSettings": [
    "-dev.containers.copyGitConfig",
    "-dotfiles.installCommand",
    "-dotfiles.repository",
    "-dotfiles.targetPath"
  ],
  "telemetry.telemetryLevel": "off",
  "terminal.explorerKind": "external",
  "terminal.integrated.defaultLocation": "editor",
  "terminal.integrated.fontSize": 14,
  "terminal.integrated.hideOnStartup": "whenEmpty",
  "terminal.integrated.inheritEnv": false,
  "terminal.integrated.initialHint": false,
  "terraform.codelens.referenceCount": true,
  "terraform.experimentalFeatures.prefillRequiredFields": true,
  "terraform.experimentalFeatures.validateOnSave": true,
  "typescript.surveys.enabled": false,
  "update.showReleaseNotes": false,
  "window.commandCenter": false,
  "window.confirmSaveUntitledWorkspace": false,
  "window.newWindowProfile": "Default",
  "window.title": "${activeRepositoryBranchName}${separator}${activeEditorShort}${separator}${rootName}${separator}${profileName}",
  "workbench.colorTheme": "Visual Studio Light",
  "workbench.editor.empty.hint": "hidden",
  "workbench.editor.enablePreview": false,
  "workbench.editor.scrollToSwitchTabs": true,
  "workbench.editor.showTabIndex": true,
  "workbench.editor.tabActionCloseVisibility": false,
  "workbench.editor.wrapTabs": true,
  "workbench.preferredDarkColorTheme": "Visual Studio Dark",
  "workbench.preferredLightColorTheme": "Visual Studio Light",
  "workbench.secondarySideBar.showLabels": false,
  "workbench.startupEditor": "none",
  "yaml.schemas": {
    "file://~/.vscode/extensions/continue.continue-1.2.11-darwin-arm64/config-yaml-schema.json": [
      ".continue/**/*.yaml"
    ]
  }
}
