{
  "chat.editor.fontSize": 14,
  "continue.enableTabAutocomplete": true,
  "continue.telemetryEnabled": false,
  "debug.console.fontSize": 14,
  "docsYaml.telemetry.enableTelemetry": false,
  "editor.fontSize": 14,
  "editor.renderWhitespace": "all",
  "editor.stickyScroll.enabled": true,
  "editor.tabSize": 2,
  "editor.wordWrap": "on",
  "extensions.ignoreRecommendations": true,
  "files.insertFinalNewline": true,
  "files.trimTrailingWhitespace": true,
  "git.enableCommitSigning": true,
  "github.copilot.editor.enableAutoCompletions": false,
  "github.copilot.enable": {
    "*": false,
    "helm": true,
    "ini": false,
    "javascript": true,
    "markdown": false,
    "plaintext": false,
    "scminput": false,
    "terraform": true,
    "typescript": true,
    "yaml": false
  },
  "githubPullRequests.defaultCreateOption": "createDraft",
  "githubPullRequests.defaultMergeMethod": "squash",
  "githubPullRequests.focusedMode": "overview",
  "githubPullRequests.ignoredPullRequestBranches": [
    "main",
    "master"
  ],
  "githubPullRequests.pullBranch": "always",
  "githubPullRequests.quickDiff": true,
  "githubPullRequests.showPullRequestNumberInTree": true,
  "githubPullRequests.useReviewMode": {
    "closed": false,
    "merged": false
  },
  "gitlens.codeLens.enabled": false,
  "gitlens.telemetry.enabled": false,
  "go.survey.prompt": false,
  "markdownlint.customRules": [
    "{docsmsft.docs-linting}/markdownlint-custom-rules/rules.js"
  ],
  "pgsql.resultsFontSize": 14,
  "redhat.telemetry.enabled": false,
  "scm.inputFontSize": 14,
  "telemetry.telemetryLevel": "off",
  "terminal.integrated.defaultLocation": "editor",
  "terminal.integrated.fontSize": 14,
  "terraform.codelens.referenceCount": true,
  "terraform.experimentalFeatures.prefillRequiredFields": true,
  "terraform.experimentalFeatures.validateOnSave": true,
  "typescript.surveys.enabled": false,
  "update.showReleaseNotes": false,
  "window.autoDetectColorScheme": true,
  "workbench.editor.empty.hint": "hidden",
  "workbench.preferredDarkColorTheme": "Visual Studio Dark",
  "workbench.preferredLightColorTheme": "Visual Studio Light",
  "workbench.startupEditor": "none",
  "yaml.schemas": {
    "~/.vscode/extensions/continue.continue-1.0.21-darwin-arm64/config-yaml-schema.json": [
      ".continue/**/*.yaml"
    ]
  },
  "chat.commandCenter.enabled": false,
  "git.openRepositoryInParentFolders": "never",
  "git.replaceTagsWhenPull": true
}
