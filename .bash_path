export PATH="/usr/local/bin:${PATH}"

if [       -e "/usr/local/opt/curl/bin" ]; then
  export PATH="/usr/local/opt/curl/bin:${PATH}"
fi
if [       -e "/usr/local/opt/openssl/bin" ]; then
  export PATH="/usr/local/opt/openssl/bin:${PATH}"
fi
if [             -e "/Applications/Visual Studio Code.app/Contents/Resources/app/bin" ]; then
  export PATH="$PATH:/Applications/Visual Studio Code.app/Contents/Resources/app/bin"
fi
if [             -e "$HOME/Library/Python/3.8/bin" ]; then
  export PATH="$PATH:$HOME/Library/Python/3.8/bin"
fi
if [             -e "$HOME/bin" ]; then
  export PATH="$PATH:$HOME/bin"
fi
if [             -e "/opt/homebrew/bin" ]; then
  export PATH="$PATH:/opt/homebrew/bin"
fi

export GOPATH="${HOME}/go"
# "${GOROOT}/bin:${GOPATH}/bin:/usr/local/go/bin"
