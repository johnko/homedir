HashKnownHosts no

Host 127.0.0.1
    User vagrant
    IdentitiesOnly yes
    IdentityFile ~/.vagrant.d/insecure_private_key
    IdentityFile ~/.ssh/id_rsa
    PasswordAuthentication no
    UserKnownHostsFile /dev/null
    StrictHostKeyChecking no

Host github.com
    # Port 7999
    User git
    IdentitiesOnly yes
    IdentityFile ~/.ssh/id_rsa
    PasswordAuthentication no
