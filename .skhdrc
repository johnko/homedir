# ctrl + alt + cmd - 1 : echo # mission control space left
# ctrl + alt + cmd - 2 : echo # mission control space right
ctrl + alt + cmd - 3 : open -a "Mission Control"
ctrl + alt + cmd - 4 : open -a "Mission Control" --args 1
ctrl + alt + cmd - 5 : yabai -m space --toggle padding
ctrl + alt + cmd - 6 : osascript ~/.osascripts/terminal-toggle-sudo.scpt

# float left
ctrl + alt + cmd - q : yabai -m window --grid 101:52:1:0:25:100
# ctrl + alt + cmd - w : echo # arrow up
# float right
ctrl + alt + cmd - e : yabai -m window --grid 101:52:26:0:25:100
ctrl + alt + cmd - r : yabai -m space --rotate 270
f15 :                  osascript ~/.osascripts/toggle-terminal.scpt
ctrl + alt + cmd - t : osascript ~/.osascripts/toggle-terminal.scpt
ctrl + alt + cmd - y : yabai --restart-service && skhd --restart-service

# ctrl + alt + cmd - a : echo # arrow left
# ctrl + alt + cmd - s : echo # arrow down
# ctrl + alt + cmd - d : echo # arrow right
ctrl + alt + cmd - f : osascript ~/.osascripts/toggle-firefox.scpt
ctrl + alt + cmd - g : osascript ~/.osascripts/toggle-googlechrome.scpt
ctrl + alt + cmd - h : osascript ~/.osascripts/toggle-cursor.scpt

# float bottom left
ctrl + alt + cmd - z : yabai -m window --grid 101:52:1:50:25:50
# float bottom right
ctrl + alt + cmd - x : yabai -m window --grid 101:52:26:50:25:50
ctrl + alt + cmd - c : osascript ~/.osascripts/toggle-cursor.scpt
ctrl + alt + cmd - v : osascript ~/.osascripts/toggle-vscode.scpt
ctrl + alt + cmd - b : osascript ~/.osascripts/toggle-brave.scpt

ctrl + alt + cmd - i : open -a "iPhone Mirroring"
ctrl + alt + cmd - o : open -a "System Settings"
ctrl + alt + cmd - p : osascript ~/.osascripts/toggle-1password.scpt
ctrl + alt + cmd - k : osascript ~/.osascripts/toggle-signal.scpt
ctrl + alt + cmd - l : osascript ~/.osascripts/toggle-line.scpt

###################################################################
# grid format is <rows>:<cols>:<start-x>:<start-y>:<width>:<height>
###################################################################

ctrl + alt + cmd - up : yabai -m window --grid 101:52:1:0:50:50
ctrl + alt + cmd - down : yabai -m window --grid 101:52:1:50:50:50
# float 1st slot bottom screen
ctrl + alt + cmd - left :      yabai -m window --grid 101:52:0:50:13:50
# float 4th slot bottom screen
ctrl + alt + cmd - right :     yabai -m window --grid 101:52:39:50:13:50

# almost fullscreen
# --grid 52:52:1:1:50:50
# toggle float
f19 :                    yabai -m window --toggle float
ctrl + alt + cmd - tab : yabai -m window --toggle float
shift + ctrl + alt + cmd - tab : yabai -m window --ratio abs:0.75

# 0x32 is `
shift + ctrl + alt + cmd - 0x32 : yabai -m window --ratio abs:0.25
