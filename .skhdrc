shift + ctrl + alt + cmd - 3 : echo
shift + ctrl + alt + cmd - 4 : echo
# float tall
shift + ctrl + alt + cmd - 5 : yabai -m window --grid 24:24:5:1:14:22
# float tall wide 16/24 of screen for editing
shift + ctrl + alt + cmd - 6 : yabai -m window --grid 24:24:4:2:16:20

# float top left
shift + ctrl + alt + cmd - q : yabai -m window --grid 2:2:0:0:1:1
# shift + ctrl + alt + cmd - w : echo # arrow up
# float top right
shift + ctrl + alt + cmd - e : yabai -m window --grid 2:2:1:0:1:1
# float middle center quarter
shift + ctrl + alt + cmd - r : yabai -m window --grid 4:4:1:1:2:2
shift + ctrl + alt + cmd - t : osascript ~/.osascripts/sound-reset-left-right-balance.scpt
shift + ctrl + alt + cmd - y : yabai --restart-service

# shift + ctrl + alt + cmd - a : echo # arrow left
# shift + ctrl + alt + cmd - s : echo # arrow down
# shift + ctrl + alt + cmd - d : echo # arrow right
# almost fullscreen
shift + ctrl + alt + cmd - f : yabai -m window --toggle float --grid 60:60:1:1:58:58
# float 1/3 tall vertical screen
shift + ctrl + alt + cmd - g : yabai -m window --grid 3:1:0:0:1:1
shift + ctrl + alt + cmd - h : bash ~/bin/yabai_cycle_clockwise.sh

# float tall wide 18/24 of screen for focus
shift + ctrl + alt + cmd - z : yabai -m window --grid 24:24:3:3:18:21
# float tall wide 20/24 of screen for video
shift + ctrl + alt + cmd - x : yabai -m window --grid 24:24:2:4:20:19
shift + ctrl + alt + cmd - c : echo
shift + ctrl + alt + cmd - v : yabai -m space --balance
# float 2/3 tall vertical screen
shift + ctrl + alt + cmd - b : yabai -m window --grid 3:1:0:1:1:2

# grid format is <rows>:<cols>:<start-x>:<start-y>:<width>:<height>

