
# shift + ctrl + alt + cmd - 1 : echo
# shift + ctrl + alt + cmd - 2 : echo
shift + ctrl + alt + cmd - 3 : osascript ~/.osascripts/terminal-toggle-audio.scpt
shift + ctrl + alt + cmd - 4 : osascript ~/.osascripts/terminal-nosleep.scpt
shift + ctrl + alt + cmd - 5 : osascript ~/.osascripts/terminal-brew-prune.scpt
shift + ctrl + alt + cmd - 6 : yabai -m space --balance

# float top left
ctrl + alt + cmd - 1 :         yabai -m window --grid 99:60:1:0:29:49
shift + ctrl + alt + cmd - q : yabai -m window --grid 99:60:1:0:29:49
# shift + ctrl + alt + cmd - w : echo # arrow up
# float top right
ctrl + alt + cmd - 2 :         yabai -m window --grid 99:60:30:0:29:49
shift + ctrl + alt + cmd - e : yabai -m window --grid 99:60:30:0:29:49
# float middle center quarter
shift + ctrl + alt + cmd - r : yabai -m window --grid 4:4:1:1:2:2
# float 2nd slot down on vertical screen
shift + ctrl + alt + cmd - t : yabai -m window --grid 101:60:1:25:58:25
shift + ctrl + alt + cmd - y : yabai --restart-service

# shift + ctrl + alt + cmd - a : echo # arrow left
# shift + ctrl + alt + cmd - s : echo # arrow down
# shift + ctrl + alt + cmd - d : echo # arrow right
# almost fullscreen
f13 :                          yabai -m window --toggle float --grid 60:60:1:1:58:58
ctrl + alt + cmd - tab :       yabai -m window --toggle float --grid 60:60:1:1:58:58
shift + ctrl + alt + cmd - f : yabai -m window --toggle float --grid 60:60:1:1:58:58
# float 3rd slot down on vertical screen
shift + ctrl + alt + cmd - g : yabai -m window --grid 101:60:1:50:58:25
shift + ctrl + alt + cmd - h : bash ~/bin/yabai_cycle_clockwise.sh

# float tall wide 20/24 of screen for video
shift + ctrl + alt + cmd - z : yabai -m window --grid 24:24:2:4:20:19
# float tall wide 18/24 of screen for focus
f14 :                          yabai -m window --grid 24:24:3:3:18:21
shift + ctrl + alt + cmd - x : yabai -m window --grid 24:24:3:3:18:21
# float tall wide 16/24 of screen for editing
shift + ctrl + alt + cmd - c : yabai -m window --grid 24:24:4:2:16:20
# float tall
shift + ctrl + alt + cmd - v : yabai -m window --grid 24:24:5:1:14:22
# float 4th slot down on vertical screen
shift + ctrl + alt + cmd - b : yabai -m window --grid 101:60:1:75:58:25

# shift + ctrl + alt + cmd - i : echo
# shift + ctrl + alt + cmd - o : echo
# shift + ctrl + alt + cmd - p : echo
# shift + ctrl + alt + cmd - k : echo
# shift + ctrl + alt + cmd - l : echo

# grid format is <rows>:<cols>:<start-x>:<start-y>:<width>:<height>

ctrl + alt + cmd - 3 : open -a "Mission Control"
ctrl + alt + cmd - 4 : open -a "Mission Control" --args 1
ctrl + alt + cmd - 5 : osascript ~/.osascripts/terminal-brew-update.scpt
ctrl + alt + cmd - 6 : osascript ~/.osascripts/terminal-toggle-sudo.scpt

# float left
ctrl + alt + cmd - q : yabai -m window --grid 99:60:1:0:29:98
# ctrl + alt + cmd - w : echo # arrow up
# float right
ctrl + alt + cmd - e : yabai -m window --grid 99:60:30:0:29:98
# float tall center halfwide for web
ctrl + alt + cmd - r : yabai -m window --grid 99:4:1:0:2:98
f15 :                  osascript ~/.osascripts/toggle-terminal.scpt
ctrl + alt + cmd - t : osascript ~/.osascripts/toggle-terminal.scpt
ctrl + alt + cmd - y : skhd --restart-service

# ctrl + alt + cmd - a : echo # arrow left
# ctrl + alt + cmd - s : echo # arrow down
# ctrl + alt + cmd - d : echo # arrow right
ctrl + alt + cmd - f : osascript ~/.osascripts/toggle-firefox.scpt
ctrl + alt + cmd - g : osascript ~/.osascripts/toggle-googlechrome.scpt
ctrl + alt + cmd - h : yabai -m space --rotate 270

# float bottom left
ctrl + alt + cmd - z : yabai -m window --grid 99:60:1:49:29:49
# float bottom right
ctrl + alt + cmd - x : yabai -m window --grid 99:60:30:49:29:49
ctrl + alt + cmd - c : osascript ~/.osascripts/toggle-slack.scpt
ctrl + alt + cmd - v : osascript ~/.osascripts/toggle-vscode.scpt
ctrl + alt + cmd - b : osascript ~/.osascripts/toggle-brave.scpt

ctrl + alt + cmd - i : open -a "iPhone Mirroring"
ctrl + alt + cmd - o : open -a "System Settings"
ctrl + alt + cmd - p : osascript ~/.osascripts/toggle-1password.scpt
ctrl + alt + cmd - k : osascript ~/.osascripts/toggle-signal.scpt
ctrl + alt + cmd - l : osascript ~/.osascripts/toggle-line.scpt

ctrl + alt + cmd - up : yabai -m window --grid 99:60:1:0:58:49
ctrl + alt + cmd - down : yabai -m window --grid 99:60:1:49:58:49
# float 1st slot down on vertical screen
ctrl + alt + cmd - left :      yabai -m window --grid 101:60:1:0:58:25
# float 3/4 vertical screen
ctrl + alt + cmd - right :     yabai -m window --grid 101:60:1:25:58:75

