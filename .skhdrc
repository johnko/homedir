  shift + ctrl + alt + cmd - c : osascript ~/.osascripts/terminal-toggle-audio.scpt
          ctrl + alt + cmd - v : osascript ~/.osascripts/toggle-vscode.scpt
  shift + ctrl + alt + cmd - v : osascript ~/.osascripts/terminal-vol.scpt
          ctrl + alt + cmd - b : osascript ~/.osascripts/toggle-brave.scpt
