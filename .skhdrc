########## Numbers #########################################################

#         ctrl + alt + cmd - 1 : # macOS space left
# shift + ctrl + alt + cmd - 1 : echo
#         ctrl + alt + cmd - 2 : # macOS space right
# shift + ctrl + alt + cmd - 2 : echo
          ctrl + alt + cmd - 3 : open -a "Mission Control"
# shift + ctrl + alt + cmd - 3 : echo
          ctrl + alt + cmd - 4 : open -a "Mission Control" --args 1
# shift + ctrl + alt + cmd - 4 : echo
#         ctrl + alt + cmd - 5 : # PaperWM center_window
  shift + ctrl + alt + cmd - 5 : osascript ~/.osascripts/terminal-brew-update.scpt
#         ctrl + alt + cmd - 6 : # PaperWM cycle_height
  shift + ctrl + alt + cmd - 6 : osascript ~/.osascripts/terminal-toggle-sudo.scpt

########## Row Q #########################################################

#         ctrl + alt + cmd - q : # PaperWM focus_prev
# shift + ctrl + alt + cmd - q : # PaperWM swap_left
#         ctrl + alt + cmd - w : # arrow up
# shift + ctrl + alt + cmd - w : # arrow up
#         ctrl + alt + cmd - e : # PaperWM focus_next
# shift + ctrl + alt + cmd - e : # PaperWM swap_right
#         ctrl + alt + cmd - r : # PaperWM cycle_width
# shift + ctrl + alt + cmd - r : # PaperWM reverse_cycle_width
          ctrl + alt + cmd - t : osascript ~/.osascripts/toggle-terminal.scpt
# shift + ctrl + alt + cmd - t : echo
#         ctrl + alt + cmd - y : # PaperWM restart_spoons
  shift + ctrl + alt + cmd - y : skhd --restart-service

